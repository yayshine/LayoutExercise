<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yayshine.layoutexercise" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GoogleDocsActivity"
            android:label="@string/title_activity_google_docs" >
        </activity>
        <activity
            android:name=".EightTracksActivity"
            android:label="@string/title_activity_eight_tracks" >
        </activity>
        <activity
            android:name=".InstagramActivity"
            android:label="@string/title_activity_instagram" >
        </activity>
        <activity
            android:name=".EvernoteActivity"
            android:label="@string/title_activity_evernote" >
        </activity>
        <activity
            android:name=".SoundCloudActivity"
            android:label="@string/title_activity_sound_cloud" >
        </activity>
    </application>

</manifest>
